.bottom-bar-content{
    width: 100%;
    position: fixed;
    z-index: 1000;
    color:white;
    bottom: 0;
    display: flex;
    justify-content: space-between;
}
.bottom-bar-content .bottom-bar-item{
    width: 25%;
    display: flex;
    justify-content: center;
}
.bg-triangle{
    width: 100%;
    height: 100vh;
    background-image: url("http://47.105.220.82:8081/huadeen/img/bg-main.png");
    background-size: 160rpx 161rpx;
    background-repeat: repeat;
}
.bg-triangle-none{
    width: 100%;
    height: 100vh;
    background-color: white;
}
.btn{
    height: 50rpx;
    padding: 10rpx;
    background-color: white;
    border-radius: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    left: 50%;
    top: 50%;
}